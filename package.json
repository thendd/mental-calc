{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "test": "."
  },
  "dependencies": {
    "@nuxt/eslint": "1.9.0",
    "@nuxt/ui": "4.0.0",
    "nuxt": "^4.1.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.9.0",
    "eslint": "^9.36.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "pnpm eslint --fix"
    ]
  }
}
